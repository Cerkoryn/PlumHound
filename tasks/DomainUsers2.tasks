["DU with path to DA", "JSON", "DU_path_DA.json", "MATCH p=ShortestPath((n:Group)-[*1..]->(m:Group)) WHERE n.name STARTS WITH 'DOMAIN USERS' and m.name CONTAINS 'DOMAIN ADMINS' AND n <> m RETURN p"]
["DU Admin Rights", "JSON", "DU_Admin_Comp.json", "MATCH p=(m:Group)-[:AdminTo|MemberOf*1..]->(n:Computer) WHERE m.name STARTS WITH 'DOMAIN USERS' RETURN n.name as Computer"]
["DU Can RDP", "JSON", "DU_CanRDP_Comp.json", "match p=(g:Group)-[:CanRDP|MemberOf*1..]->(c:Computer) where g.name STARTS WITH 'DOMAIN USERS' return c.name as Computer"]
["DU with easy path to DA", "JSON", "DU_Easy_Path_DA.json", "match p=shortestpath((g:Group)-[:AdminTo|MemberOf|Owns|Contains|GenericAll*1..]->(m {highvalue:true})) where g.name starts with 'DOMAIN USERS' and g<>m return p"]
["DU Unconstrained to Computer", "JSON", "DU_Unconstrain.json", "MATCH p=shortestPath((g:Group)-[*1..]->(c:Computer {unconstraineddelegation: true}))  WHERE g.name STARTS WITH 'DOMAIN USERS' RETURN c.name as Computer"]
